{
  "position": "top",
  "modules-left": ["hyprland/workspaces"],
  "modules-center": ["clock"],
  "modules-right": ["network", "pulseaudio"],
  "clock": {
    "format": "{:%a %d %H:%M}",
    "tooltip": false,
  },
  "network": {
    "tooltip": false,
  },
  "pulseaudio": {
      "format": "{icon} {volume}%",
      "format-muted": "",
      "format-icons": {
          "default": ["", "", " "]
      },
      "tooltip": false,
  },
}
